<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>FlowSense Analyst - Read access to all FlowSense data with ability to generate reports and documentation</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>FlowSense Analyst</label>
    
    <!-- Application Access -->
    <applicationVisibilities>
        <application>FlowSense</application>
        <visible>true</visible>
    </applicationVisibilities>
    
    <!-- Apex Classes -->
    <classAccesses>
        <apexClass>FS_PerformanceService</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>FS_DiffService</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>FS_FlowMetadataService</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>FS_DocumentationService</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>FS_DashboardController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    
    <!-- Object Permissions -->
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>FS_Flow_Run__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>FS_Flow_Step__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>FS_Flow_Alert__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>FS_Flow_Change__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>FS_Flow_Document__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>FS_Flow_Analysis__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    
    <!-- Tab Visibility -->
    <tabSettings>
        <tab>FlowSense_Dashboard</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    <tabSettings>
        <tab>Flow_Version_Compare</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    <tabSettings>
        <tab>FS_Flow_Run__c</tab>
        <visibility>Visible</visibility>
